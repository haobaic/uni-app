
page {
		width: 100%;
		min-height: 100%;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.header{
		 width: 380rpx;
		 height: 50rpx;
		 display: -webkit-box;
		 display: -webkit-flex;
		 display: flex;
		 -webkit-box-align: center;
		 -webkit-align-items: center;
		         align-items: center;
		 -webkit-box-flex: 1;
		 -webkit-flex: 1;
		         flex: 1;
		 margin: 0 auto;
}
.header view{
		 margin-right: 10rpx;
}
.logo {
		height: 200rpx;
		width: 200rpx;
		margin-top: 200rpx;
		margin-left: auto;
		margin-right: auto;
		margin-bottom: 50rpx;
}
.text-area {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
}
.title {
		font-size: 36rpx;
		color: #8f8f94;
}
.tab {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.tab-item {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		font-size: 10pt;
		text-align: center;
		line-height: 72rpx;
		border-bottom: 6rpx solid #eee;
}
.con {
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		width: 750rpx;
		height: 100%;
}
.con>swiper {
		height: 100%;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
}
.tab-item.active {
		color: #252525;

		border-bottom-color: #252525
}
     /* 隐藏滚动条 */
::-webkit-scrollbar {
		width: 0;
		height: 0;
		color: transparent;
}
.red {
		width: 100%;
		height: 1200rpx;
}

